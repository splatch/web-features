<?xml version="1.0" encoding="UTF-8"?>
<ae:configurations
	xmlns="http://agavi.org/agavi/config/parts/validators/1.0"
	xmlns:ae="http://agavi.org/agavi/config/global/envelope/1.0"
	parent="%core.module_dir%/Files/config/validators.xml"
>
	<ae:configuration>
		
		<validators>
	      <validator class="Files_FilesValidator" name="id">
	        <arguments>
	          <argument>id</argument>
	        </arguments>
	        <errors>
	          <error>The parameter id must contain a valid file id</error>
	        </errors>
	        <ae:parameters>
	          <ae:parameter name="export">id</ae:parameter>
	        </ae:parameters> 
	      </validator>
		</validators>
		
	</ae:configuration>
</ae:configurations>
