<?xml version="1.0" encoding="UTF-8"?>
<ae:configurations xmlns="http://agavi.org/agavi/config/parts/autoload/1.0" xmlns:ae="http://agavi.org/agavi/config/global/envelope/1.0" parent="%core.system_config_dir%/autoload.xml">
	<ae:configuration>
		<autoload name="WebfeaturesBaseAction">%core.lib_dir%/action/WebfeaturesBaseAction.class.php</autoload>
		<autoload name="WebfeaturesBaseModel">%core.lib_dir%/model/WebfeaturesBaseModel.class.php</autoload>
		<autoload name="WebfeaturesBaseView">%core.lib_dir%/view/WebfeaturesBaseView.class.php</autoload>
		<autoload name="User">%core.lib_dir%/user/User.class.php</autoload>
		

		<!-- Custom database -->
		<autoload name="AgaviDoctrine2Database">%core.lib_dir%/AgaviDoctrine2Database.class.php</autoload>

		<!-- Model classes -->
		<autoload name="WebFeatures\Model\File">%core.model_dir%/WebFeatures/Model/File.php</autoload>
		<autoload name="WebFeatures\DAO\FileDAOModel">%core.model_dir%/WebFeatures/DAO/FileDAO.class.php</autoload>
		
		<autoload name="WebFeatures\Model\Repository">%core.model_dir%/WebFeatures/Model/Repository.php</autoload>
		<autoload name="WebFeatures\DAO\RepositoryDAOModel">%core.model_dir%/WebFeatures/DAO/RepositoryDAO.class.php</autoload>
		
		<autoload name="WebFeatures\Model\Feature">%core.model_dir%/WebFeatures/Model/Feature.php</autoload>
		<autoload name="WebFeatures\DAO\FeatureDAOModel">%core.model_dir%/WebFeatures/DAO/FeatureDAO.class.php</autoload>
		
		<autoload name="WebFeatures\Model\Bundle">%core.model_dir%/WebFeatures/Model/Bundle.php</autoload>
		<autoload name="WebFeatures\DAO\BundleDAOModel">%core.model_dir%/WebFeatures/DAO/BundleDAO.class.php</autoload>
		
		<autoload name="WebFeatures\Model\User">%core.model_dir%/WebFeatures/Model/User.php</autoload>
		<autoload name="WebFeatures\DAO\UserDAOModel">%core.model_dir%/WebFeatures/DAO/UserDAO.class.php</autoload>
	</ae:configuration>
</ae:configurations>